            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="hu.uniobuda.nik.felhasznaloi_fiuk.Staff_Main_menu" />
        </activity>
        <activity
            android:name=".Tables"
            android:label="@string/title_activity_tables"
            android:parentActivityName=".Staff_Main_menu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="hu.uniobuda.nik.felhasznaloi_fiuk.Staff_Main_menu" />
        </activity>
    </application>

</manifest>
